{
    "manifest_version": 3,
    "name": "インチをミリに変換する君",
    "version": "1.0",
    "description": "inchの数字を選択して右クリックで、mmに変換してくれます",
    "icons": {
      "48": "icons/icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }],
    "permissions": ["activeTab", "contextMenus"]
  }
  